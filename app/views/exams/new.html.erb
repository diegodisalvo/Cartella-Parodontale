                    :html => {:multipart => true}) do |f| %>
<%= hidden_field_tag 'patient_id', @patient.id %>

<div class="annotations">
  <%= label_tag 'Tipo di esame' %>
  <%= f.select(:exam_type, ["Prima visita", "follow-up"]) %>
<hr />
  <%= f.label 'Annotazioni' %><br />
  <%= f.text_area :notes, :size => "60x4"%>
</div>
